<config
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd"
>
    <type name="Magento\Cms\Model\Config\Source\Wysiwyg\Editor">
        <arguments>
            <argument name="adapterOptions" xsi:type="array">
                <item name="n1ed" xsi:type="array">
                    <item
            name="value"
            xsi:type="string"
          >EdSDK_Wysiwyg/N1EDAdapter</item>
                    <item
            name="label"
            xsi:type="string"
            translatable="true"
          >N1ED</item>
                </item>
            </argument>
        </arguments>
    </type>
    <type name="Magento\Ui\Block\Wysiwyg\ActiveEditor">
        <arguments>
            <argument name="availableAdapterPaths" xsi:type="array">
                <item name="EdSDK_Wysiwyg/N1EDAdapter" xsi:type="string" />
            </argument>
        </arguments>
    </type>
</config>
